<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="realestate.agent.model.class">Mozart\RealEstate\AgentBundle\Model\Agent</parameter>
    </parameters>

    <services>
        <service id="realestate.agent.model"
                 class="%realestate.agent.model.class%">
        </service>

        <service id="mozart_backoffice.options.section.realestate_agent" class="Mozart\RealEstate\AgentBundle\Backoffice\OptionSection">
            <tag name="mozart_backoffice.options.section" alias="realestate_agent" />
        </service>
    </services>
</container>
